<%= render 'header_admin' %>

<div class="nested_fields well-compact">
  <div class="form_group">
    <%= f.label :ディスク番号 %>
    <%= f.number_field :disk_numbre, min:1 %><br>
  </div>

  <div class="items">
    <%= f.fields_for :songs do |song| %>
      <%= render 'song_fields', f: song %>
    <% end %>
    <div class="links">
      <%= link_to_add_association "曲を追加", f, :songs %>
    </div>
  </div>
    <%= link_to_remove_association "ディスクを削除", f %>  
</div>