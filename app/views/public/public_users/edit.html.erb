　　　　　　　　　<h1>編集画面</h1>
<br>
<br>
<br>

<%= form_for @public, url: public_public_user_path(@public) do |f| %> 

  <div class ="field">
    <%= f.label :"名前(姓)" %>
    <%= f.text_field :end_user_last_name, autofocus: true %>
  </div>

  <div class ="field">
    <%= f.label :"名前(名)" %>
    <%= f.text_field :end_user_first_name, autofocus: true %>
  </div>

  <div class ="field">
    <%= f.label :"名前(カナ)" %>
    <%= f.text_field :end_user_last_kana, autofocus: true %>
  </div>

  <div class ="field">
    <%= f.label :"名前(カナ)" %>
    <%= f.text_field :end_user_first_kana, autofocus: true %>
  </div>

  <div class ="field">
    <%= f.label :"郵便番号" %>
    <%= f.text_field :zip, autofocus: true %>
  </div>

  <div class ="field">
    <%= f.label :"住所" %>
    <%= f.text_area :address, autofocus: true %>
  </div>

  <div class ="field">
    <%= f.label :"電話番号" %>
    <%= f.text_field :end_user_phone, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :"メールアドレス" %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="actions">
    <%= f.submit "更新" %>
  </div>
<% end %>